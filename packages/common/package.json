{
  "name": "common",
  "version": "0.0.0",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "main": "src/index.ts",
  "types": "src/index.ts",
  "exports": {
    ".": {
      "import": "./src/index.ts",
      "require": "./src/index.ts"
    },
    "./.eslintrc": "./.eslintrc.cjs",
    "./.prettierrc": "./.prettierrc.cjs",
    "./postcss.config": "./postcss.config.ts",
    "./tailwind.config": "./tailwind.config.ts",
    "./tsconfig.json": "./tsconfig.json",
    "./tsconfig.node.json": "./tsconfig.node.json",
    "./src/*": "./src/*",
    "./plugins/*": "./plugins/*"
  },
  "scripts": {
    "lint": "npx eslint src --fix && npx prettier --write src"
  },
  "dependencies": {
    "utils": "*"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwindcss": "^3.3.3",
    "tailwindcss-animate": "^1.0.7"
  }
}
